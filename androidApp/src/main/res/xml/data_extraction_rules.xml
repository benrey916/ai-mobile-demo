<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all content from backup -->
        <exclude domain="root" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude all content from device transfers -->
        <exclude domain="root" path="."/>
    </device-transfer>
</data-extraction-rules>
